{
  "compilerOptions": {
    "target": "es5",
    "module": "es6",
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "noEmitOnError": true,
    "sourceMap": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
    "node_modules"
  ],
}